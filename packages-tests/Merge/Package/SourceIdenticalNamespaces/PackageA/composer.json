{
    "autoload": {
        "psr-4": {
            "App\\Collection\\": ["src/collection"],
            "App\\Core\\": "src/core",
            "App\\FixedArray\\": ["src/array", "src/list"],
            "App\\Shared\\": "src/shared",
            "App\\YetAnother\\": ["src"]
        },
        "psr-0": {
            "Monolog\\": "src/",
            "Vendor\\Namespace\\": ["src/", "lib/"],
            "Vendor_Namespace_": "src/",
            "UniqueGlobalClass": "",
            "": "src/"
        },
        "classmap": ["src/", "lib/", "Something.php"],
        "files": ["src/MyLibrary/functions.php"],
        "exclude-from-classmap": ["/Tests/", "/test/", "/tests/"]
    }
}
